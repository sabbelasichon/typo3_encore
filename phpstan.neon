includes:
    - phpstan-baseline.neon
parameters:
  editorUrl: 'phpstorm://open?file=%%file%%&line=%%line%%'
  level: 8
  paths:
    - Classes
    - Tests

  inferPrivatePropertyTypeFromConstructor: true
  ignoreErrors:
    - identifier: missingType.iterableValue
  typo3:
    requestGetAttributeMapping:
        _links: array
